  "devDependencies": {
    "@changesets/cli": "2.27.5",
    "@pandacss/dev": "0.40.1",
    "@rollup/plugin-alias": "5.1.0",
    "@sweatpants/libra": "0.0.40",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "framer-motion": "11.2.9",
